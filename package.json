{
    "name": "aas-conformance-gate",
    "version": "1.0.0",
    "description": "GitHub Action for AAS conformance testing using IDTA AAS Test Engines",
    "main": "dist/index.js",
    "scripts": {
        "build": "ncc build src/main.ts -o dist --source-map --license licenses.txt",
        "test": "jest",
        "test:coverage": "jest --coverage",
        "lint": "eslint src/**/*.ts __tests__/**/*.ts",
        "lint:fix": "eslint src/**/*.ts __tests__/**/*.ts --fix",
        "typecheck": "tsc --noEmit",
        "all": "npm run lint && npm run typecheck && npm run test && npm run build"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/YOUR_ORG/aas-conformance-gate.git"
    },
    "keywords": [
        "github-action",
        "aas",
        "asset-administration-shell",
        "conformance",
        "testing",
        "idta"
    ],
    "author": "Your Name",
    "license": "MIT",
    "dependencies": {
        "@actions/core": "^1.10.1",
        "@actions/exec": "^1.1.1",
        "@actions/io": "^1.1.3",
        "fast-glob": "^3.3.2"
    },
    "devDependencies": {
        "@types/jest": "^29.5.11",
        "@types/node": "^20.10.6",
        "@typescript-eslint/eslint-plugin": "^6.17.0",
        "@typescript-eslint/parser": "^6.17.0",
        "@vercel/ncc": "^0.38.1",
        "eslint": "^8.56.0",
        "jest": "^29.7.0",
        "ts-jest": "^29.1.1",
        "typescript": "^5.3.3"
    },
    "engines": {
        "node": ">=20.0.0"
    }
}